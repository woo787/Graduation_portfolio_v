<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/bot_bt1"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/grad"
    android:orientation="vertical"
    tools:context=".SubMain">

    <TextView
        android:id="@+id/textView1"
        android:layout_width="86dp"
        android:layout_height="36dp"
        android:layout_marginLeft="36dp"
        android:layout_marginTop="40dp"
        android:fontFamily="sans-serif-medium"
        android:lineSpacingExtra="9sp"
        android:text="Home"
        android:textColor="#191919"
        android:textSize="30sp"
        android:textStyle="normal"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="216dp"
        android:layout_height="22dp"
        android:layout_marginLeft="36dp"
        android:layout_marginTop="80dp"
        android:fontFamily="sans-serif"
        android:lineSpacingExtra="5sp"
        android:text="Person who makes designs"
        android:textColor="#767676"
        android:textSize="15sp"
        android:textStyle="normal"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <de.hdodenhof.circleimageview.CircleImageView
        android:id="@+id/profileImage1"
        android:layout_width="62dp"
        android:layout_height="62dp"
        android:layout_marginLeft="262dp"
        android:layout_marginTop="40dp"
        android:layout_marginRight="36dp"
        android:src="@drawable/person1"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <de.hdodenhof.circleimageview.CircleImageView
        android:layout_width="20dp"
        android:layout_height="20dp"
        android:layout_marginLeft="304dp"
        android:layout_marginTop="40dp"
        android:layout_marginRight="36dp"
        android:src="@color/pink"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />


    <FrameLayout
        android:id="@+id/frameLayout1"
        android:layout_width="138dp"
        android:layout_height="124dp"
        android:layout_marginLeft="50dp"
        android:layout_marginTop="80dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView2">

        <ImageView
            android:id="@+id/iv1"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="@drawable/rounding" />

        <TextView
            android:id="@+id/textview3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="left|bottom"
            android:layout_marginLeft="15dp"
            android:layout_marginBottom="21dp"
            android:fontFamily="sans-serif"
            android:lineSpacingExtra="5sp"
            android:text="Speech"
            android:textColor="#191919"
            android:textSize="15sp"
            android:textStyle="normal" />

        <ImageButton
            android:layout_width="26dp"
            android:layout_height="21dp"
            android:layout_gravity="left|top"
            android:layout_marginLeft="15dp"
            android:layout_marginTop="28dp"
            android:background="@drawable/icon1" />

        <Button
            android:id="@+id/co_btn1"
            android:layout_width="10dp"
            android:layout_height="10dp"
            android:layout_gravity="right|top"
            android:layout_marginTop="32dp"
            android:layout_marginRight="14dp"
            android:background="@drawable/co_btn" />
    </FrameLayout>

    <FrameLayout
        android:id="@+id/frameLayout2"
        android:layout_width="138dp"
        android:layout_height="124dp"
        android:layout_marginLeft="20dp"
        android:layout_marginTop="80dp"
        android:layout_marginRight="36dp"
        app:layout_constraintLeft_toRightOf="@+id/frameLayout1"
        app:layout_constraintTop_toBottomOf="@+id/textView2">

        <ImageView
            android:id="@+id/iv2"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="@drawable/rounding" />

        <TextView
            android:id="@+id/textview4"
            android:layout_width="68dp"
            android:layout_height="21dp"
            android:layout_gravity="left|bottom"
            android:layout_marginLeft="15dp"
            android:layout_marginBottom="21dp"
            android:fontFamily="sans-serif"
            android:lineSpacingExtra="5sp"
            android:text="Password"
            android:textColor="#191919"
            android:textSize="15sp"
            android:textStyle="normal" />

        <ImageButton
            android:layout_width="26dp"
            android:layout_height="21dp"
            android:layout_gravity="left|top"
            android:layout_marginLeft="15dp"
            android:layout_marginTop="28dp"
            android:background="@drawable/icon2" />

        <Button
            android:id="@+id/co_btn2"
            android:layout_width="10dp"
            android:layout_height="10dp"
            android:layout_gravity="right|top"
            android:layout_marginTop="32dp"
            android:layout_marginRight="14dp"
            android:background="@drawable/co_btn" />
    </FrameLayout>

    <Button
        android:id="@+id/bot_bt"
        android:layout_width="133dp"
        android:layout_height="54dp"
        android:layout_marginLeft="50dp"
        android:layout_marginTop="41dp"
        android:background="@drawable/rounding1"
        android:fontFamily="sans-serif-medium"
        android:gravity="center_horizontal"
        android:lineSpacingExtra="4sp"
        android:paddingTop="16.5dp"
        android:text="LIVE VIEW"
        android:textColor="#191919"
        android:textSize="12sp"
        android:textStyle="normal"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/frameLayout1" />

    <FrameLayout
        android:id="@+id/frameLayout3"
        android:layout_width="288dp"
        android:layout_height="124dp"
        android:layout_marginLeft="50dp"
        android:layout_marginTop="60dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/bot_bt">

        <ImageView
            android:id="@+id/iv3"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="@drawable/rounding" />

        <TextView
            android:id="@+id/text5"
            android:layout_width="155dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="18dp"
            android:layout_marginLeft="15dp"
            android:textSize="21sp"
            android:fontFamily="sans-serif-medium"
            android:textStyle="normal"
            android:textColor="#191919"
            android:lineSpacingExtra="7sp"
            android:text="Tracking No.1"/>

        <ImageButton
            android:id="@+id/icon3"
            android:layout_width="26dp"
            android:layout_height="21dp"
            android:background="@drawable/icon3"
            android:layout_marginLeft="15dp"
            android:layout_marginTop="59dp"
            />
        <Button
            android:id="@+id/co_btn3"
            android:layout_width="10dp"
            android:layout_height="10dp"
            android:background="@drawable/co_btn"
            android:layout_marginLeft="232dp"
            android:layout_marginTop="36dp"
            />
        <TextView
            android:id="@+id/text6"
            android:layout_width="109dp"
            android:layout_height="16dp"
            android:textSize="10sp"
            android:fontFamily="sans-serif-medium"
            android:textStyle="normal"
            android:textColor="#ff0257"
            android:lineSpacingExtra="3sp"
            android:text="Unauthorized personnel"
            android:layout_marginTop="54dp"
            android:layout_marginLeft="58dp"
            app:layout_constraintTop_toBottomOf="@+id/text5"
            app:layout_constraintStart_toStartOf="@id/frameLayout3"
            />

        <TextView
            android:id="@+id/text7"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="170dp"
            android:layout_marginTop="54dp"
            android:fontFamily="sans-serif-medium"
            android:lineSpacingExtra="3sp"
            android:text="/"
            android:textColor="#191919"
            android:textSize="10sp"
            android:textStyle="normal"
            app:layout_constraintStart_toStartOf="@id/frameLayout3"
            app:layout_constraintTop_toBottomOf="@+id/text5" />

        <TextView
            android:id="@+id/text8"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="178dp"
            android:layout_marginTop="54dp"
            android:fontFamily="sans-serif-medium"
            android:lineSpacingExtra="3sp"
            android:text="welcome my master !"
            android:textColor="#4c74e4"
            android:textSize="10sp"
            android:textStyle="normal"
            app:layout_constraintStart_toStartOf="@id/frameLayout3"
            app:layout_constraintTop_toBottomOf="@+id/text5" />

        <TextView
            android:id="@+id/text9"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="58dp"
            android:layout_marginTop="76dp"
            android:fontFamily="sans-serif-medium"
            android:lineSpacingExtra="3sp"
            android:text="Stability figure : "
            android:textColor="#191919"
            android:textSize="10sp"
            android:textStyle="normal"
            app:layout_constraintStart_toStartOf="@id/frameLayout3"
            app:layout_constraintTop_toBottomOf="@+id/text5" />

        <TextView
            android:id="@+id/text10"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="134dp"
            android:layout_marginTop="76dp"
            android:fontFamily="sans-serif-medium"
            android:lineSpacingExtra="3sp"
            android:text="Danger"
            android:textColor="#ff0257"
            android:textSize="10sp"
            android:textStyle="normal"
            app:layout_constraintStart_toStartOf="@id/frameLayout3"
            app:layout_constraintTop_toBottomOf="@+id/text5" />

        <TextView
            android:id="@+id/text11"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="170dp"
            android:layout_marginTop="76dp"
            android:fontFamily="sans-serif-medium"
            android:lineSpacingExtra="3sp"
            android:text="/"
            android:textColor="#191919"
            android:textSize="10sp"
            android:textStyle="normal"
            app:layout_constraintStart_toStartOf="@id/frameLayout3"
            app:layout_constraintTop_toBottomOf="@+id/text5" />

        <TextView
            android:id="@+id/text12"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="179dp"
            android:layout_marginTop="76dp"
            android:fontFamily="sans-serif-medium"
            android:lineSpacingExtra="3sp"
            android:text="safe"
            android:textColor="#4c74e4"
            android:textSize="10sp"
            android:textStyle="normal"
            app:layout_constraintStart_toStartOf="@id/frameLayout3"
            app:layout_constraintTop_toBottomOf="@+id/text5" />

        <TextView
            android:id="@+id/text13"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="203dp"
            android:layout_marginTop="76dp"
            android:fontFamily="sans-serif-medium"
            android:lineSpacingExtra="3sp"
            android:text="/"
            android:textColor="#191919"
            android:textSize="10sp"
            android:textStyle="normal"
            app:layout_constraintStart_toStartOf="@id/frameLayout3"
            app:layout_constraintTop_toBottomOf="@+id/text5" />

        <TextView
            android:id="@+id/text14"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="212dp"
            android:layout_marginTop="76dp"
            android:fontFamily="sans-serif-medium"
            android:lineSpacingExtra="3sp"
            android:text="Super secure"
            android:textColor="#31eb69"
            android:textSize="10sp"
            android:textStyle="normal"
            app:layout_constraintStart_toStartOf="@id/frameLayout3"
            app:layout_constraintTop_toBottomOf="@+id/text5" />


    </FrameLayout>

    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="40dp"
        android:layout_marginTop="100dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/frameLayout3">

        <ImageView
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="@drawable/rectangle" />

        <ImageButton
            android:id="@+id/support_btn"
            android:layout_width="40dp"
            android:layout_height="match_parent"
            android:layout_marginLeft="64dp"
            android:background="@drawable/icon4" />

        <ImageButton
            android:layout_width="40dp"
            android:layout_height="match_parent"
            android:layout_marginLeft="165dp"
            android:background="@drawable/icon5" />

        <ImageButton
            android:id="@+id/setting"
            android:layout_width="40dp"
            android:layout_height="match_parent"
            android:layout_marginLeft="266dp"

            android:background="@drawable/icon6" />

    </FrameLayout>


</androidx.constraintlayout.widget.ConstraintLayout>